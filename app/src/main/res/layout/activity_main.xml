<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ScrollView

        android:id="@+id/ScrollView1"

        android:layout_width="match_parent"

        android:layout_height="match_parent">

        <LinearLayout

            android:layout_width="match_parent"

            android:layout_height="match_parent"

            android:orientation="vertical">


            <Button
                android:id="@+id/button"
                android:layout_width="409dp"
                android:layout_height="90dp"
                android:onClick="onButton1Clicked"
                android:text="Android Toast"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.146" />

            <Button
                android:id="@+id/button2"
                android:layout_width="403dp"
                android:layout_height="90dp"
                android:onClick="onButton2Clicked"
                android:text="종료"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="198dp" />

            <Button
                android:id="@+id/button3"
                android:layout_width="409dp"
                android:layout_height="96dp"
                android:onClick="onButton3Clicked"
                android:text="NAVER"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.472"
                tools:layout_editor_absoluteX="-2dp" />

            <Button
                android:id="@+id/button4"
                android:layout_width="406dp"
                android:layout_height="101dp"
                android:onClick="onButton4Clicked"
                android:text="TEL"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="402dp" />

            <Button
                android:id="@+id/button5"
                android:layout_width="407dp"
                android:layout_height="98dp"
                android:onClick="onButton5Clicked"
                android:text="새창 띄우기"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="515dp" />

            <Button

                android:id="@+id/button6"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:onClick="onButton6Clicked"
                android:text="Facebook 창 만들기" />

            <Button

                android:id="@+id/button7"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:onClick="onButton7Clicked"
                android:text="NAVER 창 만들기" />

            <Button

                android:id="@+id/button8"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:text="버튼8" />

            <Button

                android:id="@+id/button9"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:text="버튼9" />


        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>